.search{
  padding: 20rpx;
  background:#ff2d4a;
}
.search .search-input{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  background: #fff;
  border-radius: 5px;
  color: #bbb;
  font-size: 16px;
}
.search-input icon{
  margin-right: 16rpx;
}
.swiper{
  height: 340rpx;
}
.swiper image{
  width: 750rpx;
  height: 340rpx;
}
.menu{
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding:20rpx 0;
}
.menu image{
  width:128rpx;
  height: 128rpx;
}